{
    "name": "default",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "start-dev": "rm -rf ./dist && yarn build && yarn copy-templates && yarn start",
        "start": "node ./dist/server.js",
        "test": "./node_modules/.bin/jest --rootDir=./src",
        "copy-templates": "copyfiles -u 1 src/**/*.html ./dist"
    },
    "prettier": {
        "printWidth": 80,
        "bracketSpacing": false,
        "jsxBracketSameLine": true,
        "trailingComma": "all",
        "singleQuote": true,
        "arrowParens": "always",
        "tabWidth": 2
    },
    "dependencies": {
        "@google-cloud/connect-datastore": "^3.0.1",
        "@google-cloud/datastore": "^4.3.0",
        "@google-cloud/logging-bunyan": "^1.2.3",
        "@google-cloud/tasks": "^1.2.1",
        "@types/google-cloud__tasks": "^0.2.1",
        "bunyan": "^1.8.12",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "google-auth-library": "^5.3.0",
        "mustache-express": "^1.3.0",
        "raw-body": "^2.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-typescript": "^7.3.3",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-session": "^1.15.14",
        "@types/jest": "^24.0.17",
        "@types/mustache-express": "^1.2.1",
        "@types/node": "^12.7.1",
        "babel-jest": "^24.8.0",
        "copyfiles": "^2.1.1",
        "jest": "^24.8.0",
        "prettier": "1.18.2",
        "supertest": "^4.0.2",
        "typescript": "^3.5.3"
    }
}
